def calculate_percentage(total: int, filled: int) -> float:
    """
    Calculates the percentage of filled annotations.
    
    Args:
        total (int): Total number of images.
        filled (int): Number of images with non-empty annotations.
    
    Returns:
        float: Percentage of filled annotations.
    """
    if total == 0:
        return 0.0
    return (filled / total) * 100


if __name__ == "__main__":
    folder_path = "Europe-Car-Updated"  # Update with your folder path
    results = process_xml_files(folder_path)

    # Display results with percentage
    for filename, total, filled in results:
        percentage = calculate_percentage(total, filled)
        print(f"{filename} - {total}/{filled} ({percentage:.2f}%)")
